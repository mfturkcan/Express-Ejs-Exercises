<%- include("./partials/header") -%>

<h1>Home</h1>
<p> <%= homeStartingContent %></p>


<% posts.forEach(function(element){  var title = element.postTitle; var body;%>
  <h1><%= element.postTitle %></h1>
<% if(element.postBody.length > 100)  body = element.postBody.slice(0, 100) + "...";  else body = element.postBody;%>
<p> <%= body %> </p>
<a href= "/posts/<%= title %>" >Read More</a>
<%}); %>

<%- include("./partials/footer") -%>
